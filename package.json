{
  "name": "rlss-overlay-project",
  "version": "1.0.0",
  "description": "Rocket League SOS Overlay and Control Panel",
  "main": "start.cjs",
  "scripts": {
    "install:all": "npm install --prefix Overlay && npm install --prefix ControlPanel",
    "dev": "concurrently \"vite --config Overlay/vite.config.js\" \"vite --config ControlPanel/vite.config.js\"",
    "build": "npm run build:overlay && npm run build:controlpanel",
    "build:overlay": "vite build --config Overlay/vite.config.js",
    "build:controlpanel": "vite build --config ControlPanel/vite.config.js",
    "start": "node start.cjs",
    "package": "pkg . --out-path dist-exe"
  },
  "bin": "start.cjs",
  "author": "Dakota Carmer",
  "license": "MIT",
  "dependencies": {
    "express": "^4.19.2",
    "open": "^9.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "pkg": "^5.8.1",
    "vite": "^5.2.0"
  },
  "pkg": {
    "scripts": ["start.cjs"],
    "assets": [
      "Overlay/dist/**",
      "ControlPanel/dist/**"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
